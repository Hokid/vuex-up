{
  "name": "vuex-up",
  "version": "1.0.1",
  "description": "Extends vuex module features",
  "main": "dist/vuex-up.cjs.js",
  "module": "dist/vuex-up.esm.js",
  "browser": "dist/vuex-up.umd.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/",
    "types/",
    "es/",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist es types",
    "build-tsc": "tsc",
    "build-dist": "rollup -c",
    "build": "npm run clean && npm run build-tsc && npm run build-dist",
    "test": "mocha 'test/**/*.spec.js' -colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hokid/vuex-up.git"
  },
  "keywords": [
