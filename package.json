    "mixin",
    "vuex-services",
    "services",
    "vuex-up",
    "up"
  ],
  "author": "Kirill Khoroshilov <mr.hokid@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.18",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "rollup": "^1.9.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.0",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "typescript": "^3.2.2",
    "vue": "^2.6.10",
    "vuex": "^3.1.0"
  },
  "dependencies": {
    "deepmerge": "^3.2.0",
    "lodash.isplainobject": "^4.0.6"
